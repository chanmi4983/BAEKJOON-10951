#include <stdio.h>

int main(void) {
  int A, B, sum; //A, B 선언
  int n1, n2, n3; // A의 십의 자리 수와 n2의 일의 자리 수 
  int count= 4; // 횟수 
  
  scanf("%d""%d", &A, &B); // A, B에 수 저장해주기 
  //궁금한 점 스캔에프나 프린트에프 따옴표안 기준 
  
  while(n3 == 10*A + B ) {
      n1 = A + B; // 각각 수끼리 더한 값 
      n2 = n1%10; // 더한 값의 일의 자리수 
      n3 = 10*B + n2; // 새로운 수 
    count++ ;
      }
  
  printf("%d", count); // 조건에 해당하면 sum 구하기 
  // 궁금한점 식에서 구한 것이 그냥 둥둥 떠다니는 퍼센트디가 자연스럽게 썸에 할당이 되는 건가
  // 다음 케이스를 어떻게 해야하나
  return 0;
}
